#include <stdio.h>
#include <stdlib.h>

void GetSet(int [], int *);

int main() {
    
    int *data, num;
    GetSet(&data, &num);

    return 0;
}//End int main

void GetSet(int data[], int *num) {
    
    printf("กรอกขนาดของเซต : ");
    scanf("%d", num);
    
    data = malloc((*num) * sizeof(int));
    
    if (data == NULL) {
        fprintf(stderr, "การจองหน่วยความจำล้มเหลว\n");
        exit(EXIT_FAILURE);
    }//End if data malloc

    printf("กรอกตัวประกอบของเซต:\n");
    
    for (int i = 0; i < *num; i++) {
        printf("เลขในเซต %d: ", i + 1);
        scanf("%d", &data[i]);
    }//END for

    printf("ขนาดของเซต : %d\n", *num);
    printf("ตัวประกอบของเซต: ");
    
    for (int i = 0; i < *num; i++) {
        printf("%d ", data[i]);
    }//END for
    
}//END Void GetSet
