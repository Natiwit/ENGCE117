#include <stdio.h>

char* reverse( char str1[] );

int main() {
    char text[50] = "I Love You";
    char *out;
    out = reverse(text);
    return 0;
}//End main

char* reverse( char str1[] ) {
    int a = strlen( str1 ) ;
    for (int i = 0, j = a - 1; i < j; i++, j--) {
        char box = str1[i];
        str1[i] = str1[j];
        str1[j] = box;
    }//End for
    printf("Reversed : %s\n", str1);
    return str1;
}//End reverse
